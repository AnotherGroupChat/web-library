<script id="breadcrumbsTemplate" type="text/x-jsrender">
    {{for breadcrumbs}}
        {{if #data.path}}
        <a href="{{url:#data.path}}">{{>#data.label}}</a>
        {{else}}
        {{>#data.label}}
        {{/if}}
        {{if (#index < (breadcrumbs.length - 1))}} > {{/if}}
    {{/for}}
</script>

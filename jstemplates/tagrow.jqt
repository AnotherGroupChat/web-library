<script id="tagrowTemplate" type="text/x-jsrender">
{{if ~showAutomatic || apiObj.meta.type == 0}}
<li class="tag-row" data-tagtitle="{{attr:apiObj.tag}}">
    <a class='tag-link' title="{{attr:apiObj.tag}} {{if apiObj.meta.numItems}}({{attr:apiObj.meta.numItems}}){{/if}}" data-tagtitle="{{attr:apiObj.tag}}" href="{{url:newurl}}">{{>~trimString(apiObj.tag, 12)}}</a>
</li>
{{/if}}
</script>

<script id="breadcrumbsTemplate" type="text/x-jquery-tmpl">
    {{each(index, crumb) breadcrumbs}}
        {{if crumb.href}}
        <a href="${crumb.href}">${crumb.label}</a>
        {{else}}
        ${crumb.label}
        {{/if}}
        {{if (index < (breadcrumbs.length - 1))}} > {{/if}}
    {{/each}}
</script>
